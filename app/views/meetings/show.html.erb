<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @meeting.name %>
</p>

<p>
  <strong>Start time:</strong>
  <%= @meeting.start_time %>
</p>

<p>
  <strong>End time:</strong>
  <%= @meeting.end_time %>
</p>

<p>
  <strong>Description:</strong>
  <%= @meeting.description %>
</p>

<p>
  <strong>User:</strong>
  <%= @meeting.user_id %>
</p>

<h2>Register for One-On-One Help</h2>

<% if @meeting.counselings.count == 1 %>
<p style="color: #ff0000">Closed</p>

<% else %>
<%= render 'counseling_signup' %>
<% end %>

<%= link_to 'Edit', edit_meeting_path(@meeting) %> |
<%= link_to 'Back', meetings_path %>
